generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WeatherInput {
  id          Int      @id @default(autoincrement())
  email       String
  name        String
  precipTotal Float    @map("precip_total")
  location    String
  picturePath String?
  enteredAt   DateTime @default(now()) @map("entered_at")

  @@map("weather_input")
}

model Admin {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String

  @@map("admin")
}
